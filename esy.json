{
  "name": "melange",
  "version": "7.2.0",
  "dependencies": {
    "ocaml": "4.14.0",
    "@opam/dune": ">= 3.5",
    "@opam/base64": ">= 3.1.0",
    "@opam/cmdliner": ">= 1.1.0",
    "@opam/ocamlfind": "1.9.5",
    "@opam/luv": "*",
    "@opam/melange-compiler-libs": "*",
    "@opam/reason": ">= 3.8.1"
  },
  "devDependencies": {
    "web-tree-sitter": "0.18.0",
    "@opam/ounit2": "*",
    "@opam/ocaml-lsp-server": "*"
  },
  "scripts": {
    "test": "dune runtest -p mel,#{self.name} --disable-promotion",
    "clean": "dune clean"
  },
  "esy": {
    "buildsInSource": "unsafe",
    "build": "dune build -p #{self.name},mel",
    "install": [
      [
        "esy-installer",
        "melange.install",
        "mel.install"
      ]
    ],
    "buildEnv": {
      "CAML_LD_LIBRARY_PATH": "#{self.target_dir / 'default' / 'jscomp' / 'stubs' : $CAML_LD_LIBRARY_PATH }"
    }
  },
  "description": "ReScript compiler for use in esy, ocaml standard libary by ReScript and its required runtime support",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melange-re/melange.git"
  },
  "keywords": [
    "ocaml",
    "rescript",
    "stdlib",
    "functional programming"
  ],
  "author": {
    "name": "Hongbo Zhang"
  },
  "maintainers": [
    {
      "name": "hongbo_zhang",
      "email": "bobzhang1988@gmail.com"
    }
  ],
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/rescript-lang/rescript-compiler/issues"
  },
  "homepage": "https://github.com/rescript-lang/rescript-compiler#readme",
  "installConfig": { "pnp": false }
}
