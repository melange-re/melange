{
  "name": "melange",
  "version": "7.2.0",
  "dependencies": {
    "ocaml": "4.12.0",
    "@opam/cmdliner": "*",
    "@opam/reason": "^3.7.0",
    "@opam/dune": "*",
    "@opam/dune-action-plugin": "*",
    "@opam/melange-compiler-libs": "melange-re/melange-compiler-libs:melange-compiler-libs.opam#74916dd9"
  },
  "devDependencies": {
    "@opam/ocaml-lsp-server": "ocaml/ocaml-lsp:ocaml-lsp-server.opam",
    "web-tree-sitter": "0.18.0",
    "@opam/merlin": "*",
    "@opam/ounit2": "*"
  },
  "scripts": {
    "test": "esy b dune runtest --ignore-promoted-rules",
    "clean": "node ./scripts/ninja.js clean"
  },
  "esy": {
    "build": "dune build -p #{self.name}",
    "install": [
      ["esy-installer", "$cur__target_dir/default/melange.install"],
      ["$cur__root/fix-install.sh"]
    ],
    "exportedEnv": {
      "BSLIB": {
        "val": "#{self.lib / 'melange'}",
        "scope": "global"
      }
    }
  },
  "description": "ReScript compiler for use in esy, ocaml standard libary by ReScript and its required runtime support",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melange-re/melange.git"
  },
  "keywords": ["ocaml", "rescript", "stdlib", "functional programming"],
  "author": {
    "name": "Hongbo Zhang"
  },
  "maintainers": [
    {
      "name": "hongbo_zhang",
      "email": "bobzhang1988@gmail.com"
    }
  ],
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/rescript-lang/rescript-compiler/issues"
  },
  "homepage": "https://github.com/rescript-lang/rescript-compiler#readme",
  "installConfig": {
    "pnp": false
  }
}
