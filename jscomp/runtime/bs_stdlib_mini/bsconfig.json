{
  "name": "@melange/runtime-stdlib-mini",
  "version": "7.0.0",
  "bsc-flags": [
    "-no-keep-locs",
    "-no-alias-deps",
    "-bs-no-version-header",
    "-bs-no-check-div-by-zero",
    "-nostdlib",
    "-bs-cross-module-opt",
    "-nopervasives",
    "-unsafe"
  ],
  "warnings": {
    "error": "+A",
    "number": "+50-20"
  },
  "sources": [
    {
      "dir": "."
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "generate-merlin": false
}
