(library
 (name bsb)
 (preprocess
  (action
   (run %{bin:cppo} %{env:CPPO_FLAGS=} %{input-file})))
 (modules :standard \ bsb_config)
 (modules_without_implementation bsb_config_types)
 (flags
  (:standard -w -9-50))
 (libraries bsb_config bsb_helper bs_hash_stubs ext common str unix base64))

