(library
 (name ext)
 (wrapped false)
 (flags
  (:standard -w -9 -open Melange_compiler_libs))
 (libraries unix bs_hash_stubs melange-compiler-libs))

(rule
 (targets ext_string.ml)
 (deps ext_string.cppo.ml)
 (action
  (run cppo -D TYPE_STRING %{env:CPPO_FLAGS=} %{deps} -o %{targets})))

